{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    // "logo": {
    //     "padding": {
    //         "top": 0
    //     },
    //     "source": "./img/earth.gif",
    //     "type": "iterm",
    //     "width": 20
    // },
    "display": {
        "separator": "  ",
        "color": {
            "keys": "magenta"
        },
        "size": {
            "ndigits": 0,
            "maxPrefix": "MB"
        },
        "key": {
            "type": "icon"
        }
    },
    "modules": [
        {
            "type": "title",
            "color": {
                "user": "green",
                "at": "red",
                "host": "blue"
            },
            "format": "{user-name-colored} {at-symbol-colored} {host-name-colored}"
        },
        "os",
        "kernel",
        "memory",
        "packages",
        "uptime",
        {
            "type": "colors",
            "key": "Colors",
            "symbol": "circle"
        }
    ]
}
